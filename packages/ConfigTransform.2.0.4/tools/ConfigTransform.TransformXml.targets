<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" DefaultTargets="TransformXml" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <UsingTask TaskName="TransformXml" AssemblyFile=".\Microsoft.Web.Publishing.Tasks.dll"/>

  <Target Name="TransformXml">
    <TransformXml 
      Source="$(Source)"
      Transform="$(Transform)"
      Destination="$(Destination)"
      Condition=""
      StackTrace="false"
      />
    
  </Target>
</Project>